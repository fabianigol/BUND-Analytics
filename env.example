# Supabase
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key

# Calendly (OAuth 2.0 - REQUERIDO)
# ⚠️ OAuth 2.0 es requerido. API Key ya no es soportada.
CALENDLY_CLIENT_ID=your_calendly_client_id
CALENDLY_CLIENT_SECRET=your_calendly_client_secret
CALENDLY_REDIRECT_URI=http://localhost:3000/api/integrations/calendly/callback
CALENDLY_WEBHOOK_SECRET=your_calendly_webhook_secret
# NOTA: CALENDLY_API_KEY ha sido eliminado - solo se soporta OAuth 2.0

# Shopify
SHOPIFY_SHOP_DOMAIN=your-shop.myshopify.com
SHOPIFY_ACCESS_TOKEN=your_shopify_access_token
SHOPIFY_API_VERSION=2024-01

# Meta (Facebook) Marketing API
META_ACCESS_TOKEN=your_meta_access_token
META_AD_ACCOUNT_ID=act_your_ad_account_id
META_APP_SECRET=your_meta_app_secret

# Google Analytics (OAuth 2.0)
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret
GOOGLE_REDIRECT_URI=http://localhost:3000/api/integrations/analytics/callback
GOOGLE_ANALYTICS_PROPERTY_ID=your_ga4_property_id

# Airtable
AIRTABLE_API_KEY=your_airtable_api_key
AIRTABLE_BASE_ID=your_airtable_base_id

# Acuity Scheduling
ACUITY_USER_ID=your_acuity_user_id
ACUITY_API_KEY=your_acuity_api_key
ACUITY_WEBHOOK_SECRET=your_acuity_webhook_secret

# App
NEXT_PUBLIC_APP_URL=http://localhost:3000

